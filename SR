#! /usr/local/bin/vvp
:ivl_version "10.1 (stable)" "(v10_1-95-g9486187)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x21c0e60 .scope module, "testSR" "testSR" 2 35;
 .timescale 0 0;
v0x21d2db0_0 .net "X", 0 0, v0x2188d70_0;  1 drivers
v0x21d2e70_0 .net "Y", 0 0, v0x21d2890_0;  1 drivers
v0x21d2f40_0 .var "clk", 0 0;
v0x21d3040_0 .var "s", 3 0;
v0x21d3110_0 .net "so", 3 0, v0x21d2c30_0;  1 drivers
S_0x21c0fe0 .scope module, "shift" "ShiftRows" 2 43, 2 5 0, S_0x21c0e60;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "inarray"
    .port_info 1 /INPUT 1 "clk"
    .port_info 2 /OUTPUT 4 "outarray"
    .port_info 3 /OUTPUT 1 "X"
    .port_info 4 /OUTPUT 1 "Y"
P_0x21c1870 .param/l "dimension" 0 2 13, +C4<00000000000000000000000000000010>;
v0x2188d70_0 .var "X", 0 0;
v0x21d2890_0 .var "Y", 0 0;
v0x21d2950_0 .net "clk", 0 0, v0x21d2f40_0;  1 drivers
v0x21d2a20_0 .var "counter", 1 0;
v0x21d2b00_0 .net "inarray", 3 0, v0x21d3040_0;  1 drivers
v0x21d2c30_0 .var "outarray", 3 0;
E_0x21c19f0 .event posedge, v0x21d2950_0;
    .scope S_0x21c0fe0;
T_0 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x21d2a20_0, 0, 2;
    %end;
    .thread T_0;
    .scope S_0x21c0fe0;
T_1 ;
    %wait E_0x21c19f0;
    %load/vec4 v0x21d2a20_0;
    %addi 1, 0, 2;
    %store/vec4 v0x21d2a20_0, 0, 2;
    %load/vec4 v0x21d2a20_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_1.0, 4;
    %load/vec4 v0x21d2b00_0;
    %parti/s 1, 2, 3;
    %assign/vec4 v0x2188d70_0, 0;
    %load/vec4 v0x21d2b00_0;
    %parti/s 1, 0, 2;
    %assign/vec4 v0x21d2890_0, 0;
T_1.0 ;
    %load/vec4 v0x21d2a20_0;
    %pad/u 32;
    %cmpi/e 2, 0, 32;
    %jmp/0xz  T_1.2, 4;
    %load/vec4 v0x21d2890_0;
    %ix/load 4, 3, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x21d2c30_0, 4, 5;
T_1.2 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x21c0e60;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x21d2f40_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x21c0e60;
T_3 ;
    %delay 10, 0;
    %load/vec4 v0x21d2f40_0;
    %nor/r;
    %store/vec4 v0x21d2f40_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x21c0e60;
T_4 ;
    %vpi_call 2 51 "$display", "ShiftOut | StateIn | X | Y" {0 0 0};
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x21d3040_0, 0, 4;
    %delay 200, 0;
    %vpi_call 2 53 "$display", "%b | %b ", v0x21d3110_0, v0x21d3040_0 {0 0 0};
    %vpi_call 2 54 "$display", "%b", v0x21d2db0_0 {0 0 0};
    %vpi_call 2 55 "$display", "%b", v0x21d2e70_0 {0 0 0};
    %vpi_call 2 57 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 3;
